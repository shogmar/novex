{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "8.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.5",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.17",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/apache-pack": "^1.0.1",
        "symfony/asset": "^6.0",
        "symfony/cache": "^6.2.7",
        "symfony/config": "^6.2.7",
        "symfony/console": "^6.2.7",
        "symfony/dependency-injection": "^6.2.7",
        "symfony/dotenv": "^6.2.7",
        "symfony/event-dispatcher": "^6.2.7",
        "symfony/flex": "2.2.5",
        "symfony/form": "^6.2.7",
        "symfony/framework-bundle": "^6.2.7",
        "symfony/http-foundation": "^6.2.7",
        "symfony/http-kernel": "^6.2.7",
        "symfony/messenger": "^6.2.7",
        "symfony/mime": "^6.2.7",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/property-access": "^6.2.7",
        "symfony/property-info": "^6.2.7",
        "symfony/routing": "^6.2.7",
        "symfony/runtime": "^6.2.7",
        "symfony/security-bundle": "^6.2.7",
        "symfony/security-core": "^6.2.7",
        "symfony/security-csrf": "^6.2.7",
        "symfony/security-http": "^6.2.7",
        "symfony/serializer": "^6.2.7",
        "symfony/validator": "^6.2.7",
        "symfony/yaml": "^6.2.7"
    },
    "require-dev": {
        "symfony/browser-kit": "^6.2.7",
        "symfony/css-selector": "^6.2.7",
        "symfony/maker-bundle": "1.43.0",
        "symfony/phpunit-bridge": "^6.2.7",
        "symfony/stopwatch": "^6.2.7",
        "symfony/twig-bundle": "^6.2.7",
        "symfony/web-profiler-bundle": "^6.2.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
